{
  "services": [
    {
      "id": "nodetron",
      "source_repository": "-b master git@github.com:newrelic/demo-nodetron.git",
      "deploy_script_path": "deploy/linux/roles",
      "port": 5001,
      "destinations": ["host1"]
    }
  ],

  "global_tags": {
    "dxOwningTeam": "DEMO",
    "dxEnvironment": "development",
    "dxDepartment": "product",
    "dxProduct": "V3",
    "dxDeployedBy": "UAT Behaviors"
  },

  "resources": [
    {
      "id": "host1",
      "provider": "aws",
      "type": "ec2",
      "size": "t2.micro"
    }
  ],

  "instrumentations": {}
}
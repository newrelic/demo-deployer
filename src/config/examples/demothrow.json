{
  "services": [
    {
      "id": "nodetron1",
      "display_name": "Nodetron1",
      "source_repository": "-b master git@github.com:newrelic/demo-nodetron.git",
      "deploy_script_path": "deploy/linux/roles",
      "port": 5001,
      "destinations": ["host1", "host2", "host3"],
      "relationships": ["nodetron2"]
    },
    {
      "id": "nodetron2",
      "display_name": "Nodetron2",
      "source_repository": "-b master git@github.com:newrelic/demo-nodetron.git",
      "deploy_script_path": "deploy/linux/roles",
      "port": 5002,
      "destinations": ["host2"],
      "relationships": ["nodetron3"]
    },
    {
      "id": "nodetron3",
      "display_name": "Nodetron3",
      "source_repository": "-b master git@github.com:newrelic/demo-nodetron.git",
      "deploy_script_path": "deploy/linux/roles",
      "port": 5003,
      "destinations": ["host3"]
    }
  ],

  "resources": [
    {
      "id": "host1",
      "provider": "aws",
      "type": "ec2",
      "size": "t2.nano"
    },
    {
      "id": "host2",
      "provider": "aws",
      "type": "ec2",
      "size": "t2.nano"
    },
    {
      "id": "host3",
      "provider": "aws",
      "type": "ec2",
      "size": "t2.nano"
    },{
      "id": "spaelb1",
      "provider": "aws",
      "type": "elb",
      "listeners": ["nodetron1"]
    }
  ],

  "instrumentations": {
    "resources": [
      {
        "id": "nr_infra",
        "resource_ids": ["host1", "host2", "host3"],
        "provider": "newrelic",
        "source_repository": "-b master git@github.com:newrelic/demo-newrelic-instrumentation.git",
        "deploy_script_path": "deploy/linux/roles",
        "version": "1.5.45"
      }
    ],
    "services": [
      {
        "id": "nr_node_agent",
        "service_ids": ["nodetron1", "nodetron2", "nodetron3"],
        "provider": "newrelic",
        "source_repository": "-b master git@github.com:newrelic/demo-newrelic-instrumentation.git",
        "deploy_script_path": "deploy/node/linux/roles",
        "version": "6.11.0"
      }
    ]
  }

}